    <meta itemprop="thumbnailUrl image" content="http://media3.s-nbcnews.com/j/newscms/2016_07/1425911/160219-cruz-carson-jsw-01_ba203a6b48c0a11f68270819d9a3ed63.nbcnews-fp-1200-800.jpg" />
