<h4><a id="popular-rotator-title">Top Stories</a></h4>
<!-- Initialize the rotator -->
<script type="text/javascript">
$(document).ready(function() {
    rotatorInt = setInterval(function(){
        if ( 'jCarouselLite' in $("#popular-widget-large-carousel") && $('#popular-widget-items li').length > 1 ) {
            $("#popular-widget-large-carousel").jCarouselLite({ vertical: false, btnNext: "#popular-widget-large-forward", btnPrev: "#popular-widget-large-backward", speed: 550, scroll:1, circular:true});
            window.clearInterval(rotatorInt);
        }
    },500);
});
</script>
