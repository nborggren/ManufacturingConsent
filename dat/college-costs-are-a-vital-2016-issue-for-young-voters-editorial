      document.write('<div class="OUTBRAIN" data-src="' + outbrainurl + '" data-widget-id="AR_2" data-ob-template="LANG"></div>');
   </script>
   <script type="text/javascript" async="async" src="http://widgets.outbrain.com/outbrain.js"></script>
<!-- LANG Outbrain End -->
