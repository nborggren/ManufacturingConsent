    function create_DFM_ad(d){
        if($('#'+d).html()==''){
            var tagOrd=Math.random()*10000000000000000;
            switch(d){                
                case 'DFM-adPos-weather':     DFM_temp_Ad_string="<iframe width='120px' height='60px'  frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dweather_report&sz=120x60&c="+tagOrd+"'></iframe>"; break;
                case 'DFM-adPos-traffic':     DFM_temp_Ad_string="<iframe width='120px' height='60px'  frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dtraffic_report&sz=120x60&c="+tagOrd+"'></iframe>"; break;
                //case 'DFM-adPos-jBar-teaser': DFM_temp_Ad_string="<iframe width='300px' height='30px'  frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=kv%3Dtoolbar&sz=300x30&c="+tagOrd+"'></iframe>"; break;
                //case 'DFM-adPos-jBar-popup':  DFM_temp_Ad_string="<iframe width='300px' height='250px' frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=kv%3Dtoolbar&sz=300x250&c="+tagOrd+"'></iframe>"; break;
            }
        DFM_temp_Ad_location=d;
        setTimeout('finish_DFM_ad()',50);
        }
    setTimeout('finish_DFM_chk_el()',500);
    }
    function finish_DFM_ad(){
        $('#'+DFM_temp_Ad_location).html(DFM_temp_Ad_string);
        $("#"+DFM_temp_Ad_location+" iFrame").on("mouseover", function(){ 
            $(this).parents(".dropdown-menu").css('display', 'block');
        });
        $("#"+DFM_temp_Ad_location+" iFrame").on("mouseout", function(){ 
            $(this).parents(".dropdown-menu").css('display', '');
        });
    }
    function finish_DFM_chk_el(){ if($('#topbar').height()<100) { $('#shareBarWrapper').css('display','block') } }
    var DFM_temp_Ad_location=DFM_temp_Ad_string='';
//***** AccuWeather *****//
var siteDebug = "";
        $(function () {
            var isMetric = false;            
            var language = "en";            
            var apiUrl = "http://api.accuweather.com";
            var apiKey = "230548dfe5d54776aaaf5a1f2a19b3f5";
            var current = new Array();
            var forecasts = new Array();
            //weatherUrl = "";
            var isDayTime = false;            
            var getWeather = function (zipcode) {                
                var locationUrl = apiUrl + "/locations/v1/US/search?q=" + zipcode + "&apikey=" + apiKey;
                //var locationKey = "";
                $.ajax({
                    type: "GET",
                    url: locationUrl,
                    dataType: "jsonp",
                    cache: true,
                    jsonpCallback: "awxCallback",
                    success: function (data) {                        
                        if(data && data.length > 0) {
                            locationKey = data[0].Key;
                            //city = data[0].LocalizedName+","+data[0].AdministrativeArea.ID;                            
                            getCurrentConditions(locationKey);
                            getForecasts(locationKey);
                        } else {
                            msg = "No locations found."
                            console.log(msg);                            
                        }                                                   
                    }
                });
            };
            // Gets current conditions for the location.            
             var getCurrentConditions = function (locationKey) {
                var currentConditionsUrl = apiUrl + "/currentconditions/v1/" + locationKey + ".json?language=" + language + "&apikey=" + apiKey;
                $.ajax({
                    type: "GET",
                    url: currentConditionsUrl,
                    dataType: "jsonp",
                    cache: true,
                    jsonpCallback: "awxCallback",
                    success: function (data) {                            
                            if(data && data.length > 0) {
                                var conditions = data[0];                                   
                                var temp = isMetric ? conditions.Temperature.Metric.Value : conditions.Temperature.Imperial.Value;                                
                                current['icon'] = (conditions.WeatherIcon < 10) ? "0"+conditions.WeatherIcon : conditions.WeatherIcon;                                                                                                
                                current['weatherurl'] = conditions.Link;
                                if(siteDebug) {
                                    console.log("Current ");console.log(current);
                                }
                                //set global leaf page weather url and current condition here..
                                $('.current-weather .current-temp h5').html(temp+'&deg;')
                                $('#topbar-wrapper .currImg')   .html('<img src="http://local.dailynews.com/common/dfm/assets/img/accuweather/'+current['icon']+'.png" alt="" class="weather-icon" />');
                            }
                            else {
                                msg = "No data returned for locaton key: "+locationKey;
                                console.log(msg);
                            }                        
                    }
                });
                //console.log(current);
            };
            var getForecasts = function(locationKey) {
                var forcastUrl = apiUrl + "/forecasts/v1/daily/10day/" + locationKey + ".json?language=" + language + "&apikey=" + apiKey;
                var days = ['Sunday','Monday','Tuesday','Wednesday','Thursday','Friday','Saturday'];
                $.ajax({
                    type: "GET",
                    url: forcastUrl,
                    dataType: "jsonp",
                    cache: true,
                    //jsonpCallback: "awxCallback",
                    success: function (data) {
                            var html;
                            if(data) {                                                                
                                var forecast = data.DailyForecasts;                                
                                for(var day in forecast) {                                    
                                    if (day == 7) { break;} 
                                    var dateobj = new Date(forecast[day].Date);
                                    //var icon = isDayTime ? forecast[day].Day.Icon : forecast[day].Night.Icon;     
                                    if(day == 0) {
                                        $('#topbar-wrapper .todayshigh').html(forecast[day].Temperature.Maximum.Value + '&deg;');                                        
                                        $('#topbar-wrapper .todayslow') .html(forecast[day].Temperature.Minimum.Value  + '&deg;');
                                    } else {
                                        icon = (forecast[day].Day.Icon < 10) ? "0"+forecast[day].Day.Icon : forecast[day].Day.Icon;                                    
                                        $('#topbar-wrapper .day'+day+'Name')  .html(days[dateobj.getDay()]);
                                        $('#topbar-wrapper .day'+day+'High')  .html(forecast[day].Temperature.Maximum.Value + '&deg;');
                                        $('#topbar-wrapper .day'+day+'Img')   .html('<img src="http://local.dailynews.com/common/dfm/assets/img/accuweather/'+icon+'.png" alt="" class="weather-icon" />');
                                        $('#topbar-wrapper .day'+day+'Low')   .html(forecast[day].Temperature.Minimum.Value  + '&deg;').siblings('h6').html();
                                    }
                                }   
                                if(siteDebug) {
                                    console.log("Forecasts");console.log(forecasts);
                                }
                                //set forecast values here..
                            }
                            else {
                                msg = "No data returned for location key: "+locationKey;
                                console.log(msg);
                            }                        
                    }
                });
            };
                getWeather("90012");
            });

