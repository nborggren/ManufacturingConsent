                            <section id="body-text">
                                <div class="byline-bar">
                                        <!--start small ArticleByline1.pbo-->
<p class="byline cleanprint-byline">
  By City News Service
</p>
<!--end small ArticleByline1.pbo-->
                                    <div class="meta">
                                        <p class="timestamp">
                                            Posted: 
                                        <time datetime="">04/03/16, 7:57 AM PDT</time>
                                            <span class="divider">|</span>
                                                <time datetime="">Updated: 1 week, 1 day ago</time>
                                        </p>
                                        <a class="timestamp bylineCommentCount" href="#disqus_thread" data-disqus-identifier="dailynews-160409884"># Comments</a>
