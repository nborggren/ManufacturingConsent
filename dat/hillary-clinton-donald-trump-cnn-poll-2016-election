ce(/[\(\)\-]/g, '');}if (typeof configObj.adsection === 'undefined' && typeof window.ssid === 'string' && window.ssid.length > 0) {configObj.adsection = window.ssid;}if (CNN.VideoPlayer.apiInitialized) {window.cnnVideoManager.renderMultipleContainers({videoConfig:configObj, eventsCallback:callbackObj});} else {CNN.VideoPlayer.addVideo(configObj, callbackObj);}});</script><div class="js-media__video media__video" id="media__video_body-text_28--wrapper"><div id="body-text_28--thumbnail" class="media__video--thumbnail"><img class="media__image media__image--responsive" alt="Trump, Clinton unleash new attacks on each other" data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/160301160613-hillary-clinton-donald-trump-composite-small-169.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/160301160613-hillary-clinton-donald-trump-composite-medium-plus-169.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/160301160613-hillary-clinton-donald-trump-composite-large-169.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/160301160613-hillary-clinton-donald-trump-composite-exlarge-169.jpg" data-src-large="http://i2.cdn.turner.com/cnnnext/dam/assets/160301160613-hillary-clinton-donald-trump-composite-super-169.jpg" data-src-full16x9="http://i2.cdn.turner.com/cnnnext/dam/assets/160301160613-hillary-clinton-donald-trump-composite-full-169.jpg" data-src-mini1x1="http://i2.cdn.turner.com/cnnnext/dam/assets/160301160613-hillary-clinton-donald-trump-composite-small-11.jpg"  data-demand-load="not-loaded" data-eq-pts="mini: 0,  xsmall: 221,  small: 308,  medium: 461,  large:  781" src="data:image/gif;base64,R0lGODlhEAAJAJEAAAAAAP///////wAAACH5BAEAAAIALAAAAAAQAAkAAAIKlI+py+0Po5yUFQA7"><div class="img__preloader"></div><noscript><img alt="Trump, Clinton unleash new attacks on each other" class="media__image" src="http://i2.cdn.turner.com/cnnnext/dam/assets/160301160613-hillary-clinton-donald-trump-composite-large-169.jpg"></noscript><div a