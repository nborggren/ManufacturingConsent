var AB_AD_TEST = true;
var AB_RND_VAL = Math.floor((Math.random() * 100) + 1);
var DFP_AB_TESTS = [];

if (AB_AD_TEST) {
	

	var AD_DESIGN;

	if (AB_RND_VAL >= 0 && AB_RND_VAL < 0) {
		AD_DESIGN = 'script';
	}
	else {
		AD_DESIGN = 'gpt';
	}

	if (window['localStorage'] !== null) {
