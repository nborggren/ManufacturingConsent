(function(){
  var config_tmp;
  window.badge_header_flights = {};
  try {
    config_tmp = {};
    if (typeof config_tmp !== "undefined" && config_tmp && typeof config_tmp === "object") {
      window.badge_header_flights = config_tmp;
      
      var badge_headers = new BF_BadgeHeader();
      badge_headers.header_badges();
    }
  } catch (e) {
    console.error("Invalid badge config data!");
  }
})();
</script>

