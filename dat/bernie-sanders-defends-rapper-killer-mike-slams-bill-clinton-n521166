      }
      return 'nbcnews_fallbacksitesection';
    },
    getVideoPlayerConfig: function(){

      if (mps && mps.pagevars){
        this.videoPlayer.freewheelValue = mps.pagevars.freewheel_section;

        if(mps.pagevars.cats){
