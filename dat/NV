            <div id="calendar" class="panel-section">
                <div class="hub-header_sub-header">
                    <h2>Primaries calendar</h2>
                </div>
                <div class="panel-group">
                    <div class="row">
                        <nav class="primaries-results-nav">
                            <div class="container">
                                <div class="row">
                                    <div class="dropdown_col col-xs-6 no-border visible-xs-block visible-sm-block">
                                      <ul>
                                          <li class="has-happened">
                                            <a href='/politics/2016-election/primaries/IA'>
                                                <span class='postal-state-name'>
                                                    IA
                                                </span>
