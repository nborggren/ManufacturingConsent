<script src="http://nodeassets.nbcnews.com/scripts/94d0cf81.main.js"></script>


    <script>
        $(function () {
            var $el = $(this);
            var horizontalBarWidth = $('.horizontal-bar').width();
            $el.find('.horizontal-bar').each(function () {
                var horizontalBarSpanWidth = $(this).find('.horizontal-bar-span').width();
                var horizontalBarSpanDiff = horizontalBarWidth - horizontalBarSpanWidth;
                if (horizontalBarSpanDiff < 28) {
                    $(this).addClass('is-inverse');
                    $(this).find('.horizontal-bar-span').prepend($(this).find('.horizontal-bar-value'));
                }
            });

            var lastUpdateTS = new Date('2016-02-25T08:54:30-05:00');
            var hrsAgo = Math.floor((new Date() - lastUpdateTS) / 3600000);
            var isLive = (hrsAgo < 12); // decide if race is Live
            //console.log('### hrsAgo',isLive, hrsAgo, lastUpdateTS);
            if(isLive) {
                var $liveUpdates = $('#js-live-updates');
                $liveUpdates.show();
                var refreshIntervalSeconds = 30;
                var $liveUpdatesText = $('#js-live-updates-text');
                var remainingSeconds = refreshIntervalSeconds;

                function refreshResults() {
                    var ajaxOptions = {url:'',
                        success: function (data) {
                            var content = $(data).find('#js-state-results');
                            $('#js-state-results-wrapper').html(content);
                            remainingSeconds = refreshIntervalSeconds;
                            $liveUpdatesText.hide().fadeIn(800);
                            main.viewBindings();
                        },
                        dataType: 'html'
                    };
                    $.ajax(ajaxOptions);
                }

                setInterval(function () {
                    remainingSeconds--;
                    $liveUpdatesText.html('Results auto-refresh in '+ remainingSeconds +' seconds');
                }, 1000);

                $('.fa-refresh').on('click', refreshResults);
                setInterval(refreshResults, refreshIntervalSeconds * 1000);
            }


            $('a[href*=#]:not([href=#])').click(function() {
                if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
                    var target = $(this.hash);
                    target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
                    if (target.length) {
                        $('html,body').animate({
                            scrollTop: target.offset().top
                        }, 500);
                        return false;
                    }
                }
            });

            var numExitpolls = $('.exitpolls').length;
            if (numExitpolls > 1) {
                $('.exitpoll-jump').show();
            }
        });
    </script>


