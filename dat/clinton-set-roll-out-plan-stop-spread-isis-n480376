      type: 'post',
      articleIndex: 1,
    },
    videoPlayer: {
      freewheelValue: 'nbcnews_fallbacksitesection',
      sect:'',
      topic:'',
      show:'',
      storyline:'',
      feature:''
    },
      videoData: {
