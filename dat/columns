	<script language="JavaScript">
		<!--
		google_ad_client = 'ca-lmc-digitalfirstmedia'; // substitute the publisher's client_id
		google_ad_channel = '';
		google_language = 'en';
		google_ad_format = '728x90_pas_abgn'; 
		google_ad_width = '728';
		google_ad_height = '90';
		google_ad_type = 'text'; // type of ads to display
		google_page_url = 'http://www.dailynews.com';
