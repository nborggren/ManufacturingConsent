al cd--has-siblings cd--has-media cd--media__image" data-vr-contentbox="/videos/us/2016/03/13/donald-trump-stage-rusher-savidge-intv-nr.cnn/video/playlists/race-to-2016/" data-eq-pts="xsmall: 0, small: 300, medium: 460, large: 780, full16x9: 1100" data-video-id="us/2016/03/13/donald-trump-stage-rusher-savidge-intv-nr.cnn"><div class="cd__wrapper" data-analytics="%20(19%20Videos)_carousel-medium-strip_video_video"><div class="media"><a href="/videos/us/2016/03/13/donald-trump-stage-rusher-savidge-intv-nr.cnn/video/playlists/race-to-2016/" ><img class="media__image media__image--responsive" alt="donald trump stage rusher savidge intv nr_00012815.jpg" data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/160313181337-donald-trump-stage-rusher-savidge-intv-nr-00012815-small-169.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/160313181337-donald-trump-stage-rusher-savidge-intv-nr-00012815-medium-plus-169.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/160313181337-donald-trump-stage-rusher-savidge-intv-nr-00012815-large-169.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/160313181337-donald-trump-stage-rusher-savidge-intv-nr-00012815-exlarge-169.jpg" data-src-large="http://i2.cdn.turner.com/cnnnext/dam/assets/160313181337-donald-trump-stage-rusher-savidge-intv-nr-00012815-super-169.jpg" data-src-full16x9="http://i2.cdn.turner.com/cnnnext/dam/assets/160313181337-donald-trump-stage-rusher-savidge-intv-nr-00012815-full-169.jpg" data-src-mini1x1="http://i2.cdn.turner.com/cnnnext/dam/assets/160313181337-donald-trump-stage-rusher-savidge-intv-nr-00012815-small-11.jpg"  data-eq-pts="mini: 0,  xsmall: 221,  small: 308,  medium: 461,  large:  781" src="data:image/gif;base64,R0lGODlhEAAJAJEAAAAAAP///////wAAACH5BAEAAAIALAAAAAAQAAkAAAIKlI+py+0Po5yUFQA7"><div class="img__preloader"></div><noscript><img alt="donald trump stage rusher savidge intv nr_00012815.jpg" class="media__image" src="http://i2.cdn.turner.com/cnnnext/dam/as