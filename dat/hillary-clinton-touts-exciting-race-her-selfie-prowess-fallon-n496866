      cat: 'politics|2016-election|article',
      type: 'post',
      articleIndex: 1,
    },
    videoPlayer: {
      freewheelValue: 'nbcnews_fallbacksitesection',
      sect:'',
      topic:'',
      show:'',
      storyline:'',
      feature:''
    },
      videoData: {
        section: 'politics',
