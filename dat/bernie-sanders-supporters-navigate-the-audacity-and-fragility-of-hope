bpol"},{"headline":"Count Him Out: Paul Ryan Slams Door Shut on 2016 Bid","id":"O5JKHO6JTSE801","slug":"2016-04-12/count-him-out-paul-ryan-slams-door-shut-on-2016-bid","type":"video","url":"http://www.bloomberg.com/politics/videos/2016-04-12/count-him-out-paul-ryan-slams-door-shut-on-2016-bid","primarySite":"bpol"}],"updatedAt":"2016-02-10T22:28:10.000Z","searchPath":"/stories/search/last/groupPublishedAt/1/week","size":12,"editorialTitle":"Headband","content_types":["article","video","slideshow","breaking_news","feature","interactive","tracker"]},"/politics/api/articles/slug/2016-02-23%2Fbernie-sanders-supporters-navigate-the-audacity-and-fragility-of-hope":{"adIds":["sports","bschool"],"abstract":[],"adTargeting":{"suid":"O2ZVS96JTSGF01","page":"article","currentResource":"Story|O2ZVS96JTSGF01","ni":["ALLTOP","ALLWWCUR","BBEXCLUDE","BNALL","BPOLITICS","COMTOP","CORRECT","ELECTOP","FLOWCMS","GENTOP","GENTOPZ1","GENTOPZ2","GOV","GOVTOP","GOVTOPZ2","GOWEB","HLNOVEL","HLNOVELTY9","PHOTO","MSCIWORLD","MUNTTOP","MUNTTOPZ4","MUNTTOPZ5","MUNTTOPZ6","NORTHAM","NOVEL","NOVELTY9","NYX","ONWEB","POL","POLNEWS","READ150","READ250","READ500","SF","TOP","US","USBNX","USGOV","WWTOP","WWTOPZ5","WWTOPZ6","WORLD","WEBELIG","ATTACHMENT","BGOVBILLGO","BGOVCODES","BIZNEWS","BONDWIRES","VOD","TIMENI","TIMECO","SRCRANK1","G10MEMB","G7MEMB","SPREGIONS","INTERNAL","LEAST","MAJOR","MISC","POLIRISK","MSCINAMER","DC","ALLSTATES","PADDIST","USSE","USSO","PADD1","SC","PADD3","AL","GEN","ESGRES","CRIME","ESGCONTROV"],"tagr":["sports","bschool"],"kwl":["pol_test","pol_goldman_q2"]},"attachments":{"all":[{"id":"252849616","type":"video","placement":["lede"]},{"id":"252679712","type":"image","placement":["inline"]},{"id":"252849616","type":"video"},{"id":"252679712","type":"image"},{"id":"252762156","type":"image"},{"id":"252762156","type":"image","placement":["thumbnail"]}],"image":{"252679712":{"baseUrl":"http://assets.bwbx.io/images/iqpuI3ArjL.Q/v2/-1x-1.png","description":"","caption":"","origW