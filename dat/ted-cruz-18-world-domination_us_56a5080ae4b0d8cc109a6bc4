  var customTags = {"author":"igor bobic","vertical":"politics","article type":"slides_ajax","tags":"Ted Cruz, ted cruz 2016,2016 election, election 2016,Iowa,@instantarticle,@no_cover_image,@applenews,@health_ibs,@health_erectile,@health_models","sponsor tags":"","internal tags":"@instantarticle,@no_cover_image,@applenews,@health_ibs,@health_erectile,@health_models","slideshow present":"true","slideshow locations":"entrybody","video present":"false","video locations":"","video types":"video_novideo"};
  
  Object.keys(customTags).forEach(function (key) {
    window.optimizely.push(['customTag', key, customTags[key]]);
  });
  
  function getOptimizelyTestMap() {
    if ("undefined" != typeof window["optimizely"]) {
      var allTests = window["optimizely"].allExperiments
        , charCount = 0
        , variationMap = window["optimizely"].variationMap
        , variationMapActive = []; // Leave behind only tests that are still running
  
      for (var testId in variationMap) {
        if (variationMap.hasOwnProperty(testId)) {
          var map = []
            , mapString = ""
            , testDefined = false
            , testEnabled = false
            ;
  
          testDefined = allTests.hasOwnProperty(testId);
          testEnabled = testDefined && allTests[testId].hasOwnProperty("enabled");
  
          if (testEnabled) {
            map = [].concat(variationMap[testId]).join("_");
            mapString = "op" + testId + ":" + map;
  
            if ((charCount + mapString.length) <= 255) {
              // Only add if we won't exceed the 255 character cap
              charCount += mapString.length;
              variationMapActive.push(mapString);
            }
          }
        }
      }
      return variationMapActive;
    }
  }
</script>
