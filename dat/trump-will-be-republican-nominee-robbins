inner.setIsPlaying(false);}}},onCVPVisibilityChange: function (containerId, cvpId, visible) {CNN.VideoPlayer.handleAdOnCVPVisibilityChange(containerId, visible);}};if (typeof configObj.context !== 'string' || configObj.context.length <= 0) {configObj.context = 'opinions'.replace(/[\(\)\-]/g, '');}if (typeof configObj.adsection === 'undefined' && typeof window.ssid === 'string' && window.ssid.length > 0) {configObj.adsection = window.ssid;}if (CNN.VideoPlayer.apiInitialized) {window.cnnVideoManager.renderMultipleContainers({videoConfig:configObj, eventsCallback:callbackObj});} else {CNN.VideoPlayer.addVideo(configObj, callbackObj);}});</script><div class="js-media__video media__video" id="media__video_body-text_6--wrapper"><div id="body-text_6--thumbnail" class="media__video--thumbnail"><img class="media__image media__image--responsive" alt="Mexico&amp;#39;s ex-president drops F-bomb on Trump&amp;#39;s wall" data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/160225164913-fmr-mexico-president-vicente-fox-f-bomb-wall-trump-sot-nr-00003207-small-169.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/160225164913-fmr-mexico-president-vicente-fox-f-bomb-wall-trump-sot-nr-00003207-medium-plus-169.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/160225164913-fmr-mexico-president-vicente-fox-f-bomb-wall-trump-sot-nr-00003207-large-169.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/160225164913-fmr-mexico-president-vicente-fox-f-bomb-wall-trump-sot-nr-00003207-exlarge-169.jpg" data-src-large="http://i2.cdn.turner.com/cnnnext/dam/assets/160225164913-fmr-mexico-president-vicente-fox-f-bomb-wall-trump-sot-nr-00003207-super-169.jpg" data-src-full16x9="http://i2.cdn.turner.com/cnnnext/dam/assets/160225164913-fmr-mexico-president-vicente-fox-f-bomb-wall-trump-sot-nr-00003207-full-169.jpg" data-src-mini1x1="http://i2.cdn.turner.com/cnnnext/dam/assets/160225164913-fmr-mexico-president-vicente-fox-f-bomb-wall-trump-sot-nr-00003