var fb_click = function(){
	$$('.fb-post-share').each(function(el){
		var lnk = el.getAttribute('rel:buzz_link');
		$(el).observe('click',function(){
			window.open('https://www.facebook.com/sharer.php?u='+lnk+'','_blank','width=600,height=400');
		});
	})
}
document.observe('dom:loaded',fb_click);
</script>



