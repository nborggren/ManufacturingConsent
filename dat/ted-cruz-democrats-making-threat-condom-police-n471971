onISO":"PT2M40S"},{"format":"MPEG4","assetType":"Limelight MP4","publicUrl":"http://link.theplatform.com/s/2E2eJC/SYdUTtKdudMD","width":"640","height":"360","bitRate":"800000","privateUrl":null,"duration":"160","durationISO":"PT2M40S"},{"format":"QT","assetType":"Mezzanine Video","publicUrl":"http://link.theplatform.com/s/2E2eJC/QzQ1St1wP9HL","width":"0","height":"0","bitRate":"6000000","privateUrl":null,"duration":"160","durationISO":"PT2M40S"}],"video_playlists":[{"playlist_id":"nnd_21427708","label":null}],"nativeVideo":false,"autoPlay":false,"hideAssociatedPlaylist":false,"defaultAssociation":null,"duration":160,"liveVideoEvent":null,"liveVideoStatus":null,"hasCaption":false,"durationISO":"PT2M40.366S","captionLinks":{"requiresCaptioning":"False","clean":null,"smpte-tt":null,"srt":null,"web-vtt":null}},
