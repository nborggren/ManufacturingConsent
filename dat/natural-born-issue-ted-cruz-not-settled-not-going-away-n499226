rivateUrl":null,"duration":"145.167","durationISO":"PT2M25.167S"},{"format":"MPEG4","assetType":"Akamai Video","publicUrl":"http://link.theplatform.com/s/2E2eJC/DVl8RIbBls6M?mbr=true","width":"640","height":"360","bitRate":"985451","privateUrl":null,"duration":"145.134","durationISO":"PT2M25.134S"}],"video_playlists":[{"playlist_id":"nnd_18424734","label":null}],"nativeVideo":false,"autoPlay":false,"hideAssociatedPlaylist":false,"defaultAssociation":null,"duration":144,"liveVideoEvent":null,"liveVideoStatus":null,"hasCaption":false,"durationISO":"PT2M24.667S","captionLinks":{"requiresCaptioning":"False","clean":null,"smpte-tt":null,"srt":"http://media.snappytv.com/captions/928000/791p791/2016-01-18T23-33-49.333Z--145.134.mp4.srt","web-vtt":"http://tve_static-snappytvpoc.nbcuni.com/snappy/caption/NBC_News/501/355/2016-01-18T23-33-49.333Z--145.134.mp4.vtt"}},
