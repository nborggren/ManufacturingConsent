<script type="text/javascript">
    function removeHrefsFromNav(){
        $('.dropdown').each(function(){
        switch($(this).children('a').text()){
          case 'Tools':
          case 'Services':
          case 'Marketplace':
          case 'Special Reports':
            $(this).children('a').attr({href:'',onclick:'return false'})
            break;
        }
        })
    }
</script>
