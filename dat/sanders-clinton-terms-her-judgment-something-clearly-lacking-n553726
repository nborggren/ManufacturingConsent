8Phguwp9RA?mbr=true","width":"640","height":"360","bitRate":"990425","privateUrl":null,"duration":"136.834","durationISO":"PT2M16.834S"}],"video_playlists":[{"playlist_id":"nnd_18424744","label":null}],"nativeVideo":false,"autoPlay":false,"hideAssociatedPlaylist":false,"defaultAssociation":null,"duration":136,"liveVideoEvent":null,"liveVideoStatus":null,"hasCaption":false,"durationISO":"PT2M16.367S","captionLinks":{"requiresCaptioning":"False","clean":null,"smpte-tt":null,"srt":"http://media.snappytv.com/captions/928000/169p169/2016-04-10T13-20-06.9Z--136.833.mp4.srt","web-vtt":"http://tve_static-snappytvpoc.nbcuni.com/snappy/caption/NBC_News/382/31/2016-04-10T13-20-06.9Z--136.833.mp4.vtt"}},
