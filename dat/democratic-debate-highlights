nction (containerId, cvpId, contentId) {/* Add a VisualRevenue event on video end */if (Array.isArray(window._vrq)) {window._vrq.push(['video', 'stop', configObj.video]);}if (Modernizr && !Modernizr.phone && !Modernizr.mobile && !Modernizr.tablet) {if (typeof videoPinner !== 'undefined' && videoPinner !== null) {videoPinner.setIsPlaying(false);}}},onCVPVisibilityChange: function (containerId, cvpId, visible) {CNN.VideoPlayer.handleAdOnCVPVisibilityChange(containerId, visible);}};if (typeof configObj.context !== 'string' || configObj.context.length <= 0) {configObj.context = 'politics'.replace(/[\(\)\-]/g, '');}if (typeof configObj.adsection === 'undefined' && typeof window.ssid === 'string' && window.ssid.length > 0) {configObj.adsection = window.ssid;}if (CNN.VideoPlayer.apiInitialized) {window.cnnVideoManager.renderMultipleContainers({videoConfig:configObj, eventsCallback:callbackObj});} else {CNN.VideoPlayer.addVideo(configObj, callbackObj);}});</script><div class="js-media__video media__video" id="media__video_body-text_39--wrapper"><div id="body-text_39--thumbnail" class="media__video--thumbnail"><img class="media__image media__image--responsive" alt="democratic debate clinton sanders campaign contributions orig vstan cws 05_00013826" data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/160211223427-democratic-debate-clinton-sanders-campaign-contributions-orig-vstan-cws-05-00013826-small-169.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/160211223427-democratic-debate-clinton-sanders-campaign-contributions-orig-vstan-cws-05-00013826-medium-plus-169.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/160211223427-democratic-debate-clinton-sanders-campaign-contributions-orig-vstan-cws-05-00013826-large-169.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/160211223427-democratic-debate-clinton-sanders-campaign-contributions-orig-vstan-cws-05-00013826-exlarge-169.jpg" data-src-large="http://i2.cdn.turner.com/cn