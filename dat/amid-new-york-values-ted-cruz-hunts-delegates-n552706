ddw?mbr=true","width":"426","height":"240","bitRate":"548175","privateUrl":null,"duration":"120.134","durationISO":"PT2M0.134S"}],"video_playlists":[{"playlist_id":"nnd_18424734","label":null}],"nativeVideo":false,"autoPlay":false,"hideAssociatedPlaylist":false,"defaultAssociation":null,"duration":120,"liveVideoEvent":null,"liveVideoStatus":null,"hasCaption":false,"durationISO":"PT2M0.167S","captionLinks":{"requiresCaptioning":"False","clean":null,"smpte-tt":null,"srt":"http://media.snappytv.com/captions/928000/524p524/2016-04-07T22-33-55.767Z--120.133.mp4.srt","web-vtt":"http://tve_static-snappytvpoc.nbcuni.com/snappy/caption/NBC_News/796/746/2016-04-07T22-33-55.767Z--120.133.mp4.vtt"}},
