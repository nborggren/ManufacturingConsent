    <div class="js-video-embed form_embed-wrapper is-hidden">
      <form action="/">
        <div class="form_group">
          <label for="embed" class="font-small">Copy this code to your website or blog</label>
          <textarea id="embed" name="embed" onclick="this.focus();this.select()" autofocus="autofocus" class="form_control form_embed" rows="4" readonly="readonly"><iframe src="http://www.nbcnews.com/widget/video-embed/<%- video.mpxId %>" width="560" height="315" frameborder="0" allowfullscreen></iframe></textarea>
        </div>
      </form>    </div>
  </script>

