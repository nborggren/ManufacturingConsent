Player.handleAdOnCVPVisibilityChange(containerId, visible);}};if (typeof configObj.context !== 'string' || configObj.context.length <= 0) {configObj.context = 'politics'.replace(/[\(\)\-]/g, '');}if (typeof configObj.adsection === 'undefined' && typeof window.ssid === 'string' && window.ssid.length > 0) {configObj.adsection = window.ssid;}if (CNN.VideoPlayer.apiInitialized) {window.cnnVideoManager.renderMultipleContainers({videoConfig:configObj, eventsCallback:callbackObj});} else {CNN.VideoPlayer.addVideo(configObj, callbackObj);}});</script><div class="js-media__video media__video" id="media__video_body-text_53--wrapper"><div id="body-text_53--thumbnail" class="media__video--thumbnail"><img class="media__image media__image--responsive" alt="Sanders hits Clinton over trade" data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/160310003701-bernie-sanders-hillary-clinton-march-9-debate-small-169.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/160310003701-bernie-sanders-hillary-clinton-march-9-debate-medium-plus-169.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/160310003701-bernie-sanders-hillary-clinton-march-9-debate-large-169.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/160310003701-bernie-sanders-hillary-clinton-march-9-debate-exlarge-169.jpg" data-src-large="http://i2.cdn.turner.com/cnnnext/dam/assets/160310003701-bernie-sanders-hillary-clinton-march-9-debate-super-169.jpg" data-src-full16x9="http://i2.cdn.turner.com/cnnnext/dam/assets/160310003701-bernie-sanders-hillary-clinton-march-9-debate-full-169.jpg" data-src-mini1x1="http://i2.cdn.turner.com/cnnnext/dam/assets/160310003701-bernie-sanders-hillary-clinton-march-9-debate-small-11.jpg"  data-demand-load="not-loaded" data-eq-pts="mini: 0,  xsmall: 221,  small: 308,  medium: 461,  large:  781" src="data:image/gif;base64,R0lGODlhEAAJAJEAAAAAAP///////wAAACH5BAEAAAIALAAAAAAQAAkAAAIKlI+py+0Po5yUFQA7"><div class="img__preloader"></div><noscript><img al