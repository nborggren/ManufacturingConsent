,"height":"360","bitRate":"800000","privateUrl":null,"duration":"136","durationISO":"PT2M16S"},{"format":"QT","assetType":"Mezzanine Video","publicUrl":"http://link.theplatform.com/s/2E2eJC/cAWPzz7k53Mp","width":"0","height":"0","bitRate":"6000000","privateUrl":null,"duration":"136","durationISO":"PT2M16S"},{"format":"MPEG4","assetType":"Limelight MP4","publicUrl":"http://link.theplatform.com/s/2E2eJC/UGZfvzQAM4wv","width":"420","height":"240","bitRate":"500000","privateUrl":null,"duration":"136","durationISO":"PT2M16S"}],"video_playlists":[{"playlist_id":"nnd_18424692","label":null}],"nativeVideo":false,"autoPlay":true,"hideAssociatedPlaylist":false,"defaultAssociation":null,"duration":136,"liveVideoEvent":null,"liveVideoStatus":null,"hasCaption":false,"durationISO":"PT2M16.233S","captionLinks":{"requiresCaptioning":"False","clean":null,"smpte-tt":null,"srt":null,"web-vtt":null}},
