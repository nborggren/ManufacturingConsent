nstance.cvp || null;if (CNN.Utils.existsObject(cvp)) {cvp.reportAnalytics('videoPageTiming', {pageLoad: CNN.VideoPlayer.loadTime});}}};}captureTimeToFirstVideoFrame();CNN.VideoPlayer.showSpinner = function showSpinner(containerId) {if (Modernizr && !Modernizr.phone && !Modernizr.mobile && !Modernizr.tablet) {jQuery(document.getElementById(('spinner_' + containerId).replace('#', ''))).show();}};CNN.VideoPlayer.hideSpinner = function hideSpinner(containerId) {if (Modernizr && !Modernizr.phone && !Modernizr.mobile && !Modernizr.tablet) {jQuery(document.getElementById(('spinner_' + containerId).replace('#', ''))).hide();}};CNN.VideoPlayer.hideThumbnail = function hideThumbnail(containerId) {if (Modernizr && !Modernizr.phone && !Modernizr.mobile && !Modernizr.tablet) {jQuery(document.getElementById(containerId + '--thumbnail')).hide();}};CNN.autoPlayVideoExist = (CNN.autoPlayVideoExist === true) ? true : false;var configObj = {thumb: 'none',video: 'world/2016/03/16/china-on-donald-trump-rivers-pkg.cnn',width: '100%',height: '100%',section: 'domestic',profile: 'expansion',network: 'cnn',markupId: 'large-media_0',adsection: 'const-article-carousel-pagetop',frameWidth: '100%',frameHeight: '100%'},autoStartVideo = false,callbackObj,currentVideoCollection = [{"title":"China on Trump: Doubts and expectations ","duration":"02:32","sourceName":"CNN","sourceLink":"www.cnn.com","videoCMSUrl":"/video/data/3.0/video/world/2016/03/16/china-on-donald-trump-rivers-pkg.cnn/index.xml","videoId":"world/2016/03/16/china-on-donald-trump-rivers-pkg.cnn","videoImage":"http://i2.cdn.turner.com/cnnnext/dam/assets/160316103923-china-on-donald-trump-rivers-pkg-00022720-large-169.jpg","videoUrl":"/videos/world/2016/03/16/china-on-donald-trump-rivers-pkg.cnn/video/playlists/donald-trump/","descriptionText":"CNN's Matt Rivers looks at how China is reacting to the election process taking place in the United States. "},{"title":"Donald Trump: There is nobody more pro-Israel than I am","duration":"01:4