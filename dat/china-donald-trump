peof videoPinner !== 'undefined' && videoPinner !== null) {videoPinner.setIsPlaying(false);}}},onCVPVisibilityChange: function (containerId, cvpId, visible) {CNN.VideoPlayer.handleAdOnCVPVisibilityChange(containerId, visible);}};if (typeof configObj.context !== 'string' || configObj.context.length <= 0) {configObj.context = 'world'.replace(/[\(\)\-]/g, '');}if (typeof configObj.adsection === 'undefined' && typeof window.ssid === 'string' && window.ssid.length > 0) {configObj.adsection = window.ssid;}if (CNN.VideoPlayer.apiInitialized) {window.cnnVideoManager.renderMultipleContainers({videoConfig:configObj, eventsCallback:callbackObj});} else {CNN.VideoPlayer.addVideo(configObj, callbackObj);}});</script><div class="js-media__video media__video" id="media__video_body-text_19--wrapper"><div id="body-text_19--thumbnail" class="media__video--thumbnail"><img class="media__image media__image--responsive" alt="China reacts to the rise of Donald Trump" data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/160316074255-01-trump-0316-small-169.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/160316074255-01-trump-0316-medium-plus-169.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/160316074255-01-trump-0316-large-169.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/160316074255-01-trump-0316-exlarge-169.jpg" data-src-large="http://i2.cdn.turner.com/cnnnext/dam/assets/160316074255-01-trump-0316-super-169.jpg" data-src-full16x9="http://i2.cdn.turner.com/cnnnext/dam/assets/160316074255-01-trump-0316-full-169.jpg" data-src-mini1x1="http://i2.cdn.turner.com/cnnnext/dam/assets/160316074255-01-trump-0316-small-11.jpg"  data-demand-load="not-loaded" data-eq-pts="mini: 0,  xsmall: 221,  small: 308,  medium: 461,  large:  781" src="data:image/gif;base64,R0lGODlhEAAJAJEAAAAAAP///////wAAACH5BAEAAAIALAAAAAAQAAkAAAIKlI+py+0Po5yUFQA7"><div class="img__preloader"></div><noscript><img alt="China reacts to the rise of Donald Trump" class=