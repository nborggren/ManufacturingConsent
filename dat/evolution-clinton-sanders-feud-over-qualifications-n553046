l5hR?mbr=true","width":"426","height":"240","bitRate":"543224","privateUrl":null,"duration":"142.2","durationISO":"PT2M22.2S"}],"video_playlists":[{"playlist_id":"nnd_18424734","label":null}],"nativeVideo":false,"autoPlay":false,"hideAssociatedPlaylist":false,"defaultAssociation":null,"duration":141,"liveVideoEvent":null,"liveVideoStatus":null,"hasCaption":false,"durationISO":"PT2M21.834S","captionLinks":{"requiresCaptioning":"False","clean":null,"smpte-tt":null,"srt":"http://media.snappytv.com/captions/928000/534p534/2016-04-08T23-07-40.2Z--142.2.mp4.srt","web-vtt":"http://tve_static-snappytvpoc.nbcuni.com/snappy/caption/NBC_News/568/143/2016-04-08T23-07-40.2Z--142.2.mp4.vtt"}},
