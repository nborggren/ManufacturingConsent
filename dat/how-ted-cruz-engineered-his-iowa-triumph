ription":"","caption":"","credit":"Andre Tartar/Bloomberg","origWidth":850,"images":{"-1x-1":{"id":"dynamic","height":"-1","width":"-1","sizeType":"0","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/-1x-1.png"]},"1200x-1":{"id":"dynamic","height":"-1","width":"1200","sizeType":"0","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/1200x-1.png"]},"750x-1":{"id":"dynamic","height":"-1","width":"750","sizeType":"0","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/750x-1.png"]},"640x-1":{"id":"dynamic","height":"-1","width":"640","sizeType":"0","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/640x-1.png"]},"628x-1":{"id":"dynamic","height":"-1","width":"628","sizeType":"0","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/628x-1.png"]},"488x-1":{"id":"dynamic","height":"-1","width":"488","sizeType":"0","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/488x-1.png"]},"765x430":{"id":"250876296","height":"430","width":"765","sizeType":"63","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/765x430.png"]},"620x349":{"id":"250876294","height":"349","width":"620","sizeType":"34","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/620x349.png"]},"400x225":{"id":"250869204","height":"225","width":"400","sizeType":"18","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/400x225.png"]},"220x220":{"id":"250876292","height":"220","width":"220","sizeType":"29","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/220x220.png"]},"368x-1":{"id":"dynamic","height":"-1","width":"368","sizeType":"0","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/368x-1.png"]},"200x-1":{"id":"dynamic","height":"-1","width":"200","sizeType":"0","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/200x-1.png"]},"630x420":{"id":"250876288","height":"420","width":"630","sizeType":"66","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/630x420.png"]},"630x354":{"id":"250876286","height":"354","width":"630","sizeType":"70","url":["http://assets.bwbx.io/images/iXpz2OrOXk1Y/v1/630x