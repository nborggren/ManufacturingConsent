    <script src="https://webappstatic.buzzfeed.com/static/js/User_and_Buzz_Video_concat_footer.js?v=607239009269.406" type="text/javascript" ref:jsconf="true"></script>
     
 
 
  

    <script src="https://webappstatic.buzzfeed.com/static/js/General_concat_footer.js?v=607239009269.406" type="text/javascript" ref:jsconf="true"></script>
     
    <script type="text/javascript">
    Box.Application.init({
      debug: BF_STATIC.env !== 'live'
    });
  </script>
  
    <script type="text/javascript">
function loadScript(script) {
    var deferred = Q.defer();
    var scriptEl = document.createElement('script');
    scriptEl.setAttribute('type', 'text/javascript');
    scriptEl.setAttribute('src', script);
    scriptEl.onload = function() {
        deferred.resolve();
    };
    document.body.appendChild(scriptEl);
    return deferred.promise;
}

EventManager.observe_once(
    "acl:load:done",
    function() {
        var u = new BF_User();
        if (u) {
            u = u.getUserInfo();
        }
        if (acl.user_can('duplicate_post') || acl.user_can('feedback_tool')) {
            var scriptEl = document.createElement('script');
            scriptEl.setAttribute('type', 'text/javascript');
            scriptEl.setAttribute('src', 'https://webappstatic.buzzfeed.com/static/js/public/q/q.js');
        }

        scriptEl.onload = function() {

            if (acl.user_can('feedback_tool')) {
                Q.all([
                  loadScript('https://webappstatic.buzzfeed.com/static/js/html2canvas/html2canvas.js'),
                  loadScript('https://webappstatic.buzzfeed.com/static/js/t3/services/user.js?v=607239009269.406'),
                  loadScript('https://webappstatic.buzzfeed.com/static/js/t3/services/image-proxier.js?v=607239009269.406'),
                  loadScript('https://webappstatic.buzzfeed.com/static/js/t3/modules/feedback-widget-trigger.js?v=607239009269.406'),
                  loadScript('https://webappstatic.buzzfeed.com/static/js/t3/modules/feedback-widget.js?v=607239009269.406'),
                  loadScript('https://webappstatic.buzzfeed.com/static/js/t3/modules/screenshot-canvas.js?v=607239009269.406'),
                ])
                .then(function() {
                  var triggerEl = document.querySelector('.js-usernav-feedback');
                  triggerEl.setAttribute('data-module', 'feedback-widget-trigger');
                  Box.Application.start(triggerEl); // like init but for a single module

                  var widgetEl = document.querySelector('.js-feedback-widget');
                  widgetEl.setAttribute('data-module', 'feedback-widget');
                  Box.Application.start(widgetEl); // like init but for a single module

                  var canvasEl = document.querySelector('.js-screenshot-canvas');
                  canvasEl.setAttribute('data-module', 'screenshot-canvas');
                  Box.Application.start(canvasEl);
                });
            }
            if (acl.user_can('duplicate_post')) {
              Q.all([
                  loadScript('https://webappstatic.buzzfeed.com/static/js/t3/modules/superduper.js?v=607239009269.406')
                ])
                .then(function() {
                  var triggerEl = document.querySelector('[data-module="superduper"]');
                  Box.Application.start(triggerEl); // like init but for a single module
                });

            }
            
        }
        document.body.appendChild(scriptEl);
    }
);

</script>
  
 


<script src="https://webappstatic.buzzfeed.com/static/js/public/bpage-pinterest-nopin.js?v=607239009269.406" type="text/javascript"></script>


<script type="text/javascript">
        var uagent = this.agent = navigator.userAgent.toLowerCase();

        
        var post_success_cookie = BFW_Util.getCookie('qp_post_success');
        if (post_success_cookie && location.pathname.replace(/^\/bf2/, '').replace(/^\/h[\d]+\/h[A-Z0-9]+/i, '') == post_success_cookie) {
            $('post_launch_success').show(); BFW_Util.deleteCookie('qp_post_success');
        }
        

