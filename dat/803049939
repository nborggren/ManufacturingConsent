<script>
	placeSbarTop = function(){
		var t=$(window).scrollTop()+($('#topbar').height()-60);
		var articleOffset=$("#main-story").offset().top - t;
		var articleEnd=$("#main-story").height()-400;
		var newArticleEnd=articleEnd+$("#main-story").offset().top;
		var t=$(window).scrollTop()+($('#topbar').height()+90);
		if (articleOffset>100)    { $('#shareBarWrapper').css({'top':articleOffset,'position':'fixed','margin-top':0}); }
		else if (t>newArticleEnd) { $('#shareBarWrapper').css({'position':'absolute','margin-top':newArticleEnd}); }
		else                      { $('#shareBarWrapper').css({'top':100,'position':'fixed','margin-top':0}); }
	}
	placeSbar = function(){
		var divPos     = $("#main-story").offset();
		var sBarWidth  = $('#shareBarWrapper').outerWidth();
		var newLeftPos = divPos.left - sBarWidth -70;
		$('#shareBarWrapper').css('left', newLeftPos);
		$('#shareBarWrapper').css('display','block');
		$('#shareBarWrapper').removeClass('shareBarHide');
	}
	//placeSbarTop must run before placeSbar
	$(document).ready(placeSbarTop);
	$(document).ready(placeSbar);
	$(document).ready(function(){
		/* FireFox does not like iframes being created or moved into currently hidden elements so we had to create the twitter buttons on the page and move them into place on hover */
		$(".share-barADD .twitter").hover(
			function(){
				if($('#tempTwitterDiv').width()>0) {
					$('#tempTwitterDiv li').appendTo($('.share-barADD .twitter .share-options'));
					$('#tempTwitterDiv').remove();
				}
			},
			function(){}
		);
   });
	$(window).bind('resize', placeSbar);
	$(window).scroll('resize', placeSbarTop);
</script>
<script>
	$shareBarRegions = $(".share-barADD ul li");
	$shareBarRegions.hover(
		function(){ $(this).addClass("shareBarHover"); },
		function(){ if(!$(this).hasClass("shareBarInternalHover")){ $(this).removeClass("shareBarHover"); } }
	);
	$(".share-barADD ul li ul.share-options li").hover(
		function(){ $shareBarRegions.addClass("shareBarInternalHover"); },
		function(){ $shareBarRegions.removeClass("shareBarInternalHover"); }
	);
</script>
