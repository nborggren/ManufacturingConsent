                console.log('Begining LANG Change');
                $(document).ready(function(){
                    console.log('Changing Page Struct for LANG Sites');
                    if($('#main-story .article #extraInfo').length==0)
                        $('#main-story .article').append($('<div id="extraInfo" style="clear: both;"></div>'));
                    var extraInfo=$('#main-story .article #extraInfo');
                    $('#body-text .OUTBRAIN, #author').each(function(idx){
                        extraInfo.append($(this));
                    });
                    $('#div_Cube_Article').each(function(idx){
                        var ad=$(this);
                        var totalTopAd = ad.height() + ad.position().top
                        var lastP = $($('#body-copy p')[$('#body-copy p').length-1]);
                        var totalTopLastP = lastP.height() + lastP.position().top
                        if(totalTopAd >= totalTopLastP)
                            extraInfo.append($(ad));
                    });
                    console.log('Ending of Changing Page Struct for LANG Sites');
                });
            </script>

