S"},{"format":"MPEG4","assetType":"OnceURL","publicUrl":"http://link.theplatform.com/s/2E2eJC/dfY29JClSQf2","width":"0","height":"0","bitRate":"0","privateUrl":null,"duration":"122","durationISO":"PT2M2S"}],"video_playlists":[{"playlist_id":"nnd_21426262","label":null}],"nativeVideo":false,"autoPlay":false,"hideAssociatedPlaylist":false,"defaultAssociation":null,"duration":122,"liveVideoEvent":null,"liveVideoStatus":null,"hasCaption":false,"durationISO":"PT2M2.556S","captionLinks":{"requiresCaptioning":"False","clean":null,"smpte-tt":null,"srt":null,"web-vtt":null}},
