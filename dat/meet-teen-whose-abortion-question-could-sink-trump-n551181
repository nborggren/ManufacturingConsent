                            nextPlaylist : [],
                            nextVideoIndex : 0,
                            isLoaded : false
                          };
                      
                          window.NEWS = window.NEWS || { };
                          NEWS.articleHasVideo = true;
                          NEWS.videoInstances = NEWS.videoInstances || [];
