<script>
    $(document).ready(function(){
        removeHrefsFromNav();
        setTimeout("$('#topbar, .weather-bar, .traffic-bar').removeClass('zero-height').hide()", 750);
    });
    function create_DFM_ad(d){
        if($('#'+d).html()==''){
            var tagOrd=Math.random()*10000000000000000;
            switch(d){                
                case 'DFM-adPos-weather':     DFM_temp_Ad_string="<iframe width='120px' height='60px'  frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dweather_report&sz=120x60&c="+tagOrd+"'></iframe>"; break;
                case 'DFM-adPos-traffic':     DFM_temp_Ad_string="<iframe width='120px' height='60px'  frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dtraffic_report&sz=120x60&c="+tagOrd+"'></iframe>"; break;
                //case 'DFM-adPos-jBar-teaser': DFM_temp_Ad_string="<iframe width='300px' height='30px'  frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=kv%3Dtoolbar&sz=300x30&c="+tagOrd+"'></iframe>"; break;
                //case 'DFM-adPos-jBar-popup':  DFM_temp_Ad_string="<iframe width='300px' height='250px' frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=kv%3Dtoolbar&sz=300x250&c="+tagOrd+"'></iframe>"; break;
            }
        DFM_temp_Ad_location=d;
        setTimeout('finish_DFM_ad()',50);
        }
    setTimeout('finish_DFM_chk_el()',500);
    }
    function finish_DFM_ad(){
        $('#'+DFM_temp_Ad_location).html(DFM_temp_Ad_string);
        $("#"+DFM_temp_Ad_location+" iFrame").on("mouseover", function(){ 
            $(this).parents(".dropdown-menu").css('display', 'block');
        });
        $("#"+DFM_temp_Ad_location+" iFrame").on("mouseout", function(){ 
            $(this).parents(".dropdown-menu").css('display', '');
        });
    }
