<!-- script to generate data for the rotator -->
<script>
var siteDebug = "";
var parentProfileList = "";
DEFAULT_IMAGE = "http://local.dailynews.com/common/dfm/assets/img/top-stories-rotator/default-saxo.jpg";
$(function() {
    var most_pop_list = 'Assembly candidates square off over northeast San Fernando Valley bullet train and more~!~/government-and-politics/20160409/assembly-candidates-square-off-over-northeast-san-fernando-valley-bullet-train-and-more~!~http://image.dailynews.com/storyimage/LA/20160409/NEWS/160409484/TS/0/TS-160409484.jpg&exactW=150&exactH=108~#~';
    if(siteDebug) {    
        console.log('Call with main profile:');
        console.log('<p bs:contentstats useobjects=1 interval="7days" Objectclass=30 count=20 synchid=1 Categories="%" types="stories" Profile="1030378">');
    }
    most_pop_list = most_pop_list.split("~#~");
    /* if there aren't enough stories to pull for MainProfileID, step up in the profile hierarchy */
    if(most_pop_list.length < 7) {
        /* get parent profileID */
        var parentProfileArray = parentProfileList.split(";");
        var parentCount = parentProfileArray.length;
        /* last item is empty, 2nd last is mainProfileID, 3rd last is parentProfileID */
        var parentProfile = parentProfileArray[parentCount-3];
        /* pull stories for parent profile */
        var most_pop_list = '&#x2018;The Best Man Wedding&#x2019; delayed indefinitely, director says~!~/arts-and-entertainment/20160407/the-best-man-wedding-delayed-indefinitely-director-says~!~http://image.dailynews.com/storyimage/LA/20160407/NEWS/160409607/AR/0/AR-160409607.jpg&exactW=150&exactH=108~#~Lakers&#x2019; Kobe Bryant credits his Fusionetics treatment for getting him near the finish line~!~/sports/20160410/lakers-kobe-bryant-credits-his-fusionetics-treatment-for-getting-him-near-the-finish-line~!~http://image.dailynews.com/storyimage/LA/20160410/SPORTS/160419963/AR/0/AR-160419963.jpg&exactW=150&exactH=108~#~7 killed in Zion National Park flood honored with bench at Topanga State Park~!~/lifestyle/20160408/7-killed-in-zion-national-park-flood-honored-with-bench-at-topanga-state-park~!~http://image.dailynews.com/storyimage/LA/20160408/NEWS/160409531/AR/0/AR-160409531.jpg&exactW=150&exactH=108~#~Pacoima&#x2019;s Laurel Pharmacy to shut down after a half century~!~/business/20160409/pacoimas-laurel-pharmacy-to-shut-down-after-a-half-century~!~http://image.dailynews.com/storyimage/LA/20160409/NEWS/160409505/AR/0/AR-160409505.jpg&exactW=150&exactH=108~#~Was Porter Ranch gas leak a jab or a blow on housing market?~!~/business/20160410/was-porter-ranch-gas-leak-a-jab-or-a-blow-on-housing-market~!~http://image.dailynews.com/storyimage/LA/20160410/NEWS/160419988/AR/0/AR-160419988.jpg&exactW=150&exactH=108~#~It&#x2019;s high time to let the Libertarian presidential candidate debate: Susan Shelley~!~/opinion/20160407/its-high-time-to-let-the-libertarian-presidential-candidate-debate-susan-shelley~!~http://image.dailynews.com/storyimage/LA/20160407/LOCAL1/160409631/AR/0/AR-160409631.jpg&exactW=150&exactH=108~#~Max Browne is leader in USC quarterback derby~!~/sports/20160409/max-browne-is-leader-in-usc-quarterback-derby~!~http://image.dailynews.com/storyimage/LA/20160409/SPORTS/160409483/AR/0/AR-160409483.jpg&exactW=150&exactH=108~#~Mother, daughter and dog killed by