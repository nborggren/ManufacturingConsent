       var mediaCenterCodeLoadTester = self.setInterval(
           function(){
               if(typeof mc_embed_gallery != 'undefined'){
                   mc_embed_gallery.initWidget();
                   window.clearInterval(mediaCenterCodeLoadTester);
               }
           },100
       )
     }
</script>
