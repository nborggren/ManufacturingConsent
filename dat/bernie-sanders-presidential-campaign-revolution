ainerId);},onContentEnd: function (containerId, cvpId, contentId) {/* Add a VisualRevenue event on video end */if (Array.isArray(window._vrq)) {window._vrq.push(['video', 'stop', configObj.video]);}if (Modernizr && !Modernizr.phone && !Modernizr.mobile && !Modernizr.tablet) {if (typeof videoPinner !== 'undefined' && videoPinner !== null) {videoPinner.setIsPlaying(false);}}},onCVPVisibilityChange: function (containerId, cvpId, visible) {CNN.VideoPlayer.handleAdOnCVPVisibilityChange(containerId, visible);}};if (typeof configObj.context !== 'string' || configObj.context.length <= 0) {configObj.context = 'politics'.replace(/[\(\)\-]/g, '');}if (typeof configObj.adsection === 'undefined' && typeof window.ssid === 'string' && window.ssid.length > 0) {configObj.adsection = window.ssid;}if (CNN.VideoPlayer.apiInitialized) {window.cnnVideoManager.renderMultipleContainers({videoConfig:configObj, eventsCallback:callbackObj});} else {CNN.VideoPlayer.addVideo(configObj, callbackObj);}});</script><div class="js-media__video media__video" id="media__video_body-text_19--wrapper"><div id="body-text_19--thumbnail" class="media__video--thumbnail"><img class="media__image media__image--responsive" alt="Bernie Sanders&amp;#39; brother is also running for office" data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/150507124841-larry-sanders-thumbnail-small-169.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/150507124841-larry-sanders-thumbnail-medium-plus-169.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/150507124841-larry-sanders-thumbnail-large-169.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/150507124841-larry-sanders-thumbnail-exlarge-169.jpg" data-src-large="http://i2.cdn.turner.com/cnnnext/dam/assets/150507124841-larry-sanders-thumbnail-super-169.jpg" data-src-full16x9="http://i2.cdn.turner.com/cnnnext/dam/assets/150507124841-larry-sanders-thumbnail-full-169.jpg" data-src-mini1x1="http://i2.cdn.turner.com/cnnnext/dam/