dernizr.tablet) {if (typeof videoPinner !== 'undefined' && videoPinner !== null) {videoPinner.setIsPlaying(false);}}},onCVPVisibilityChange: function (containerId, cvpId, visible) {CNN.VideoPlayer.handleAdOnCVPVisibilityChange(containerId, visible);}};if (typeof configObj.context !== 'string' || configObj.context.length <= 0) {configObj.context = 'politics'.replace(/[\(\)\-]/g, '');}if (typeof configObj.adsection === 'undefined' && typeof window.ssid === 'string' && window.ssid.length > 0) {configObj.adsection = window.ssid;}if (CNN.VideoPlayer.apiInitialized) {window.cnnVideoManager.renderMultipleContainers({videoConfig:configObj, eventsCallback:callbackObj});} else {CNN.VideoPlayer.addVideo(configObj, callbackObj);}});</script><div class="js-media__video media__video" id="media__video_body-text_51--wrapper"><div id="body-text_51--thumbnail" class="media__video--thumbnail"><img class="media__image media__image--responsive" alt="Bill Clinton describes Obama&amp;#39;s term as an &amp;#39;awful legacy&amp;#39; " data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/160321230204-bill-clinton-obama-legacy-spokane-washington-sot-kxly-00000420-small-169.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/160321230204-bill-clinton-obama-legacy-spokane-washington-sot-kxly-00000420-medium-plus-169.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/160321230204-bill-clinton-obama-legacy-spokane-washington-sot-kxly-00000420-large-169.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/160321230204-bill-clinton-obama-legacy-spokane-washington-sot-kxly-00000420-exlarge-169.jpg" data-src-large="http://i2.cdn.turner.com/cnnnext/dam/assets/160321230204-bill-clinton-obama-legacy-spokane-washington-sot-kxly-00000420-super-169.jpg" data-src-full16x9="http://i2.cdn.turner.com/cnnnext/dam/assets/160321230204-bill-clinton-obama-legacy-spokane-washington-sot-kxly-00000420-full-169.jpg" data-src-mini1x1="http://i2.cdn.turner.com/cnnnext/dam/as