<script type="text/javascript">

cnn_shareconfig.push({
    "id" : "cnn_sharebar2",
    "url" : "http://www.cnn.com/interactive/2015/12/opinions/2016-predictions/",
    "title" : "2016 crystal ball"
});

function cnnintTweet (t_id, t_site, score, link) {

		if(window.console) {
				console.log('clicked ' + t_site + ' in: ' + t_id);
		}


		/* trigger share actions */
		var t_bar = cnnshare.share.data.bars[t_id];
		var t_social = cnnshare.sites[t_site];

		var tweet = 'http://twitter.com/intent/tweet?text=' + encodeURIComponent(score + " makes some predictions for 2016 on @cnnopinion. Read them here: " + link) + '&related=cnn,cnnopinion';


		cnnshare.share.popup(tweet, t_site, t_social.popwidth, t_social.popheight, t_social.popscroll);

		/* track click action */
		cnnshare.share.track({ 'type' : 'click', 'site' : t_site });

}

function shareFb (name, link, resultImg) {
		var builtUrl = "http://www.facebook.com/dialog/feed?display=popup&app_id=80401312489&link=" + encodeURIComponent(link) + "&name=" + encodeURIComponent('CNN Opinion - Predictions for 2016') + "&picture=" + encodeURIComponent(resultImg) + "&description=" + encodeURIComponent(name + " makes some predictions for 2016.") + "&redirect_uri=" + encodeURIComponent("http://www.cnn.com/");

		cnnshare.share.popup(builtUrl, "facebook", 550, 420, 'no');
		cnnshare.share.track({
				"type": "click",
				"site": "facebook"
		});

};
</script>

