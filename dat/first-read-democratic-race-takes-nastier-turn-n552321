  mpscall.site = ads.mpscall.site;
  mpscall.path = ads.mpscall.path;
  mpscall.cat = ads.mpscall.cat;
  mpscall.type = ads.mpscall.type;
  mpscall['cag[entryno]'] = ads.mpscall.articleIndex;

